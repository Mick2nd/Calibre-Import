{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["src", "doc", "README.md"],
	"entryPointStrategy": "expand",
	"out": "docs",
	
	"name": "Calibre Import",
	"theme": "default", 
	"basePath": ".",
	"skipErrorChecking": true,
    "plugin": ["typedoc-umlclass"],
    "umlClassDiagram": 
    {
        "type": "detailed",
        "location": "local",
        "format": "svg"
    }
}
